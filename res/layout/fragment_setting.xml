    <com.guohao.custom.SettingBg
        android:id="@+id/id_setting05"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        SettingBg:img="@drawable/img339"
        SettingBg:text="退出" />

    <View
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="#E0E0E0" />

    <com.guohao.custom.SettingBg
        android:id="@+id/id_setting06"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        SettingBg:img="@drawable/img301"
        SettingBg:text="关于" />

