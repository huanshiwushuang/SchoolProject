<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:SettingBg="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <com.guohao.custom.SettingBg
        android:id="@+id/id_setting01"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        SettingBg:img="@drawable/img104"
        SettingBg:text="版本更新" />

    <View
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="#E0E0E0" />

    <com.guohao.custom.SettingBg
        android:id="@+id/id_setting02"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        SettingBg:img="@drawable/img340"
        SettingBg:text="意见反馈" />

    <View
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="#E0E0E0" />

    <com.guohao.custom.SettingBg
        android:id="@+id/id_setting03"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        SettingBg:img="@drawable/img51"
        SettingBg:text="修改密码" />

    <View
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="#E0E0E0" />

    <com.guohao.custom.SettingBg
        android:id="@+id/id_setting04"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
